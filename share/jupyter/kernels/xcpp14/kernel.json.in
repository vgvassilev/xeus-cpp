{
  "display_name": "C++14",
  "env": {
      "CPLUS_INCLUDE_PATH":"@CMAKE_CPLUS_INCLUDE_PATH@",
      "PATH":"@CMAKE_PATH@",
      "LD_LIBRARY_PATH":"@CMAKE_LD_LIBRARY_PATH@",
      "PYTHONPATH":"@CMAKE_PYTHONPATH@"
  },
  "argv": [
      "@CMAKE_VENV_PATH@/bin/xcpp",
      "-f",
      "{connection_file}",
      "-std=c++14",
      "-fno-exceptions",
      "-O2",
      "-v"@CMAKE_OMP@
  ],
  "language": "C++14",
  "metadata": {"debugger": false}
}
